{
  "python.jediEnabled": false,
  "java.format.enabled": true,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
         "cache": {
           "directory": ".ccls-cache"
         }
       }
    },
    "kls": {
      "command": "/home/filo/lsp/KotlinLanguageServer/server-0.1.13/bin/server",
      "filetypes": ["kotlin"]
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  },
  "snippets.ultisnips.directories": [
      "/home/filo/.vim/UltiSnips"
],
"java.errors.incompleteClasspath.severity": "ignore",
"java.format.settings.url": "/home/filo/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml"
}
